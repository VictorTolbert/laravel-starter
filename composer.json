{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "algolia/algoliasearch-client-php": "^1.20",
        "barryvdh/laravel-debugbar": "^3.0",
        "bugsnag/bugsnag-laravel": "^2.7",
        "consoletvs/charts": "^5.1",
        "fideloper/proxy": "~3.3",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/date": "^3.2",
        "ksubileau/color-thief-php": "^1.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/commonmark": "^0.15.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^2.1",
        "nexmo/client": "^1.0",
        "nothingworks/blade-svg": "^0.2.1",
        "pbmedia/laravel-ffmpeg": "^1.1",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-activitylog": "^2.0",
        "spatie/laravel-analytics": "^3.1",
        "spatie/laravel-backup": "^5.0",
        "spatie/laravel-blade-javascript": "^2.0",
        "spatie/laravel-collection-macros": "^3.0",
        "spatie/laravel-demo-mode": "^2.0",
        "spatie/laravel-google-calendar": "^2.0",
        "spatie/laravel-googletagmanager": "^2.3",
        "spatie/laravel-html": "^2.4",
        "spatie/laravel-image-optimizer": "^1.1",
        "spatie/laravel-medialibrary": "^6.0.0",
        "spatie/laravel-menu": "^3.0",
        "spatie/laravel-missing-page-redirector": "^2.0",
        "spatie/laravel-newsletter": "^4.1",
        "spatie/laravel-permission": "^2.5",
        "spatie/laravel-referer": "^1.3",
        "spatie/laravel-responsecache": "^4.0",
        "spatie/laravel-sitemap": "^3.0",
        "spatie/laravel-sluggable": "^2.0",
        "spatie/laravel-tags": "^2.0",
        "spatie/laravel-tail": "^2.0",
        "spatie/laravel-translation-loader": "^2.0",
        "spatie/laravel-uptime-monitor": "^3.0",
        "spatie/regex": "^1.1",
        "themsaid/laravel-mail-preview": "^1.2",
        "tightenco/ziggy": "^0.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "spatie/phpunit-watcher": "^1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
